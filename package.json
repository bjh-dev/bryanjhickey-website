{
  "name": "bryanjhickey.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "next:dev": "next dev --turbopack",
    "next:build": "bun run sanity:typegen && next build",
    "next:start": "next start",
    "next:debug": "NEXT_PRIVATE_DEBUG_CACHE=1 next:build && next:start",
    "lint": "next lint",
    "dev": "bun run next:dev & bun run watch-typegen",
    "built": "bun run next:build",
    "start": "bun run next:start",
    "sanity:typegen": "mkdir -p ./.sanity && sanity schema extract --path=.sanity/schema.json && sanity typegen generate",
    "typecheck": "tsc --noEmit",
    "watch-typegen": "tsx watch-typegen.ts",
    "prepare": "husky",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-navigation-menu": "^1.2.12",
    "@radix-ui/react-slot": "^1.2.2",
    "@sanity/icons": "^3.7.0",
    "@sanity/image-url": "^1.1.0",
    "@sanity/vision": "^4.0.0",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fathom-client": "^3.7.2",
    "framer-motion": "^12.10.4",
    "lucide-react": "^0.532.0",
    "next": "15.4.4",
    "next-sanity": "^10.0.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-headings": "^3.3.3",
    "react-icons": "^5.5.0",
    "sanity": "^4.0.0",
    "sanity-plugin-asset-source-unsplash": "^3.0.3",
    "sanity-plugin-media": "^3.0.2",
    "sonner": "^2.0.3",
    "styled-components": "^6.1.18",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.5",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.0.0",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.26.0",
    "eslint-config-next": "15.4.4",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.5",
    "tsx": "^4.19.4",
    "typescript": "^5.8.0",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --cache --fix",
    "*.{ts,tsx,js,css,md}": "prettier --write"
  }
}
