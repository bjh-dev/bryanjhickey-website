{
  "name": "bryanjhickey.com",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.30.0",
  "scripts": {
    "next:dev": "next dev --turbopack",
    "next:build": "next build",
    "next:start": "next start",
    "next:debug": "NEXT_PRIVATE_DEBUG_CACHE=1 next:build && next:start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "dev": "pnpm run next:dev",
    "built": "pnpm exec sanity typegen generate & pnpm run next:build",
    "start": "pnpm run next:start",
    "sanity:typegen": "mkdir -p ./.sanity && NODE_OPTIONS='--require ./scripts/react-global.cjs' sanity schema extract --path=.sanity/schema.json && sanity typegen generate",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:ci": "vitest run --coverage --reporter=verbose"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-slot": "^1.2.4",
    "@sanity/client": "^7.15.0",
    "@sanity/code-input": "^7.0.8",
    "@sanity/icons": "^3.7.4",
    "@sanity/image-url": "^2.0.2",
    "@sanity/ui": "^3.1.11",
    "@sanity/vision": "^5.0.0",
    "@vercel/analytics": "^1.6.1",
    "caniuse-lite": "^1.0.30001760",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "fathom-client": "^3.7.2",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.575.0",
    "next": "16.1.6",
    "next-sanity": "^12.0.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "sanity": "^5.0.0",
    "sanity-plugin-asset-source-unsplash": "^7.0.0",
    "sanity-plugin-media": "^4.1.0",
    "sonner": "^2.0.7",
    "styled-components": "^6.3.10",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@eslint/eslintrc": "^3.3.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/estree": "^1.0.8",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^25.0.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^4.0.0",
    "eslint": "^9.39.3",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "rollup": "^4.53.3",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.19",
    "vitest": "^4.0.15"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --cache --fix",
    "*.{ts,tsx,js,css,md}": "prettier --write"
  }
}
