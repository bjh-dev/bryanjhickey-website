{
  "name": "bryanjhickey.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "next:dev": "next dev --turbopack",
    "next:build": "bun run sanity:typegen && next build",
    "next:start": "next start",
    "next:debug": "NEXT_PRIVATE_DEBUG_CACHE=1 next:build && next:start",
    "lint": "next lint",
    "dev": "bun run next:dev & bun run watch-typegen",
    "built": "bun run next:build",
    "start": "bun run next:start",
    "sanity:typegen": "mkdir -p ./.sanity && sanity schema extract --path=.sanity/schema.json && sanity typegen generate",
    "typecheck": "tsc --noEmit",
    "watch-typegen": "tsx watch-typegen.ts",
    "prepare": "husky",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:ci": "vitest run --coverage --reporter=verbose"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@sanity/icons": "^3.7.4",
    "@sanity/image-url": "^1.1.0",
    "@sanity/vision": "^4.5.0",
    "@vercel/analytics": "^1.5.0",
    "caniuse-lite": "^1.0.30001737",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fathom-client": "^3.7.2",
    "framer-motion": "^12.23.12",
    "lucide-react": "0.541.0",
    "next": "15.5.0",
    "next-sanity": "^10.0.13",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-headings": "^3.3.3",
    "react-icons": "^5.5.0",
    "sanity": "^4.5.0",
    "sanity-plugin-asset-source-unsplash": "^4.0.0",
    "sanity-plugin-media": "^4.0.0",
    "sonner": "^2.0.7",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "rollup": "^4.48.0",
    "@types/estree": "^1.0.8",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^5.4.19",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --cache --fix",
    "*.{ts,tsx,js,css,md}": "prettier --write"
  }
}
